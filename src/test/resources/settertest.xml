<?xml version="1.0" encoding="UTF-8"?>
<suite name="Suite second" parallel="tests" verbose="10">
    <test name="Parameter Test one">
        <classes>
            <parameter name="name" value="Flow" />
            <class name="getsettests.PersonSetterTests">
                <methods>
                    <include name="testSetFirstNamePositive" />
                    <include name="testSetLastNamePositive" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="Parameter Test two">
        <classes>
            <parameter name="yyyy" value="2020"></parameter>
            <parameter name="mm" value="1"></parameter>
            <parameter name="dd" value="1"></parameter>
            <class name="getsettests.PersonSetterTests">
                <methods>
                    <include name="testSetDateOfDeathPositive" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="Parameter Test tree">
        <classes>
            <parameter name="name1" value="Hanna"></parameter>
            <parameter name="name2" value="Luiza"></parameter>
            <class name="getsettests.PersonSetterTests">
                <methods>
                    <include name="testPersonSetPartnerPositive" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="Parameter Test four">
        <classes>
            <parameter name="status" value="IS_MARRIED"></parameter>
            <class name="getsettests.PersonSetterTests">
                <methods>
                    <include name="testPersonSetStatusPositive" />
                </methods>
            </class>
        </classes>
    </test>
</suite>